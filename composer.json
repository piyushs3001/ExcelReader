{
    "name": "piyush/excel-importer",
    "description": "A lightweight PHP package to import Excel files (.xlsx and .xls) with data type support",
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/piyushs3001/ExcelReader",
    "authors": [
        {
            "name": "Piyush",
            "email": "piyush@example.com"
        }
    ],
    "support": {
        "issues": "https://github.com/piyushs3001/ExcelReader/issues",
        "source": "https://github.com/piyushs3001/ExcelReader"
    },
    "keywords": [
        "excel",
        "xlsx",
        "xls",
        "import",
        "spreadsheet",
        "parser",
        "reader"
    ],
    "require": {
        "php": ">=7.2",
        "ext-xml": "*",
        "ext-simplexml": "*"
    },
    "suggest": {
        "ext-zip": "Required for reading .xlsx files (not needed for .xls files)"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Piyush\\ExcelImporter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Piyush\\ExcelImporter\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
